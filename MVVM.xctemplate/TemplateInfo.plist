platform :ios, &apos;9.0&apos;

def shared_pods

    # Network
    pod &apos;Moya/RxSwift&apos;, &apos;~&gt; 10.0&apos;
    pod &apos;ObjectMapper&apos;, &apos;~&gt; 3.0&apos;
